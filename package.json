{
  "name": "@2gis/mapgl-terra-draw",
  "description": "TerraDraw adapter for MapGL JS API",
  "version": "0.4.0-styling.0",
  "type": "module",
  "main": "dist/mapgl-terra-draw.cjs",
  "module": "dist/mapgl-terra-draw.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "start": "vite demo",
    "build": "npm run build:lib && npm run build:types && npm run build:es6",
    "build:lib": "vite -c vite.config.lib.js build",
    "build:types": "tsc --declaration --emitDeclarationOnly",
    "build:es6": "tsc",
    "build:demo": "vite build demo",
    "build:docs": "tsx utils/docs.ts",
    "prettier": "prettier --check \"./**/*..{ts,html,css,md}\"",
    "prettier:write": "prettier --write \"./**/*.{ts,html,css,md}\""
  },
  "keywords": [
    "mapgl",
    "terradraw"
  ],
  "author": "Michel Beloshitsky <itano@yandex.ru>",
  "license": "MIT",
  "devDependencies": {
    "@documentalist/compiler": "^5.0.0",
    "@types/geojson": "^7946.0.16",
    "@types/node": "^24.3.0",
    "prettier": "^3.6.0",
    "tsx": "^4.20.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@2gis/mapgl": "^1.63.0",
    "geojson": "^0.5.0",
    "terra-draw": "^1.8.0",
    "vanilla-colorful": "^0.7.2"
  }
}
